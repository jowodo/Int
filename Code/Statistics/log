get(samples,position,value):
    array={}
    for i in samples: 
        if i[position]=value: 
            array.add(i)

for each combination of vDOC and TDOC print 
for vDOC in {10,12,14,16,18,20}:
    for TDOC in {20,40,60,80}:
        samples = get(get(all_samples(9,vDOC)),10,TDOC)
        mean = samples.mean()
        variance = samples.variance()
